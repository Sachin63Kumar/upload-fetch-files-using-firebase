{
    "version": 2,
    "functions": {
      "api/*.js": {
        "runtime": "nodejs16.x",
        "maxDuration": 10
      }
    },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  